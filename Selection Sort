#include<stdio.h>
#include<conio.h>
void main()
{ clrscr();
 int a[10], i, j, temp;
 printf("enter 10 numbers to sort:\n");
 for(i=0; i<10; i++)
 scanf("%d",&a[i]);
 printf("numbers after selection sorting:\n");
 for(i=0; i<10; i++)
 {
  for(j=0; j<10; j++)
   {
    if(a[j]>a[i])
     {
      temp=a[j];
      a[j]=a[i];
      a[i]=temp;  }   } }
 for(i=0; i<10; i++)
 printf("%d\t", a[i]);
 getch();
 }
